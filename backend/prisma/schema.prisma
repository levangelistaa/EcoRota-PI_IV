generator client {
  provider = "prisma-client"
  output   = "./generated/client"
}

datasource ecorota {
  provider = "mysql"
}

model Administrador {
  id            Int      @id @default(autoincrement())
  name          String
  email         String   @unique
  password      String
  creation_date DateTime @default(now())
  update_date   DateTime @updatedAt

  @@map("administradores")
}
